{
  "name": "dwellers",
  "version": "1.0.0",
  "description": "",
  "main": "./bin/cli.js",
  "bin": {
    "fallout": "./bin/cli.js"
  },
  "files": [
    "bin/**/**"
  ],
  "scripts": {
    "start": "node ./bin/cli.js",
    "prebuild": "rimraf bin",
    "build": "npx tsc -b",
    "dev": "npx ts-node ./cli.ts",
    "test": "jest",
    "fix": "npm run lint && npm run prettier",
    "lint": "eslint --ignore-path .eslintignore --fix --ext .ts,.tsx,.js,.jsx .",
    "prettier": "prettier --ignore-path .eslintignore --write ."
  },
  "keywords": [
    "cli",
    "personal assistant"
  ],
  "author": "Rodrigo Cordeiro <rodrigomendoncca@gmail.com> (https://rodcordeiro.com.br/)",
  "private": false,
  "license": "GPL-3.0-or-later",
  "jest": {
    "testMatch": [
      "**/*/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testEnvironmentOptions": {
      "NODE_ENV": "test",
      "PORT": 3333
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!build/**",
      "!__tests__/**",
      "!*.{j,t}s"
    ],
    "coverageDirectory": "__tests__/__coverage__"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cli-table": "^0.3.11",
    "commander": "^10.0.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "inquirer": "^9.0.0",
    "knex": "^2.4.2",
    "ora": "^6.1.2",
    "update-notifier": "^6.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cli-table": "0.3.0",
    "@types/crypto-js": "4.1.1",
    "@types/inquirer": "9.0.3",
    "@types/jest": "29.4.0",
    "@types/knex": "0.16.1",
    "@types/node": "18.11.18",
    "@types/update-notifier": "6.0.1",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "cross-env": "7.0.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "sqlite3": "5.1.4",
    "ts-jest": "29.0.5",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  }
}