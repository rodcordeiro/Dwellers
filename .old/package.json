{
  "name": "dwellers",
  "version": "1.0.0",
  "description": "",
  "main": "./bin/cli.js",
  "bin": {
    "fallout": "./bin/cli.js"
  },
  "files": [
    "bin/**/**"
  ],
  "scripts": {
    "start": "node ./bin/cli.js",
    "prebuild": "rimraf bin",
    "build": "npx tsc -b",
    "dev": "npx ts-node ./cli.ts",
    "test": "jest",
    "fix": "npm run lint && npm run prettier",
    "lint": "eslint --ignore-path .eslintignore --fix --ext .ts,.tsx,.js,.jsx .",
    "prettier": "prettier --ignore-path .eslintignore --write ."
  },
  "keywords": [
    "cli",
    "personal assistant"
  ],
  "author": "Rodrigo Cordeiro <rodrigomendoncca@gmail.com> (https://rodcordeiro.com.br/)",
  "private": false,
  "license": "GPL-3.0-or-later",
  "jest": {
    "testMatch": [
      "**/*/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testEnvironmentOptions": {
      "NODE_ENV": "test",
      "PORT": 3333
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!build/**",
      "!__tests__/**",
      "!*.{j,t}s"
    ],
    "coverageDirectory": "__tests__/__coverage__"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cli-table": "^0.3.11",
    "commander": "^9.5.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "inquirer": "^8.2.5",
    "knex": "^2.4.2",
    "ora": "^6.1.2",
    "update-notifier": "^5.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cli-table": "0.3.0",
    "@types/crypto-js": "4.1.1",
    "@types/inquirer": "8.2.1",
    "@types/jest": "28.1.1",
    "@types/knex": "0.16.1",
    "@types/node": "17.0.40",
    "@types/update-notifier": "5.1.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "cross-env": "7.0.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sqlite3": "5.0.8",
    "ts-jest": "28.0.4",
    "ts-node-dev": "2.0.0",
    "typescript": "4.7.3"
  }
}